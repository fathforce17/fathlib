{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "doctrine/dbal": "^3.6.1",
        "guzzlehttp/guzzle": "^7.5",
        "intervention/image": "^2.7",
        "laravel/framework": "^10.3.3",
        "laravel/helpers": "^1.6",
        "laravel/horizon": "^5.15.0",
        "laravel/sanctum": "^3.2.1",
        "laravel/socialite": "^5.6.1",
        "laravel/tinker": "^2.8.1",
        "laravel/ui": "^4.2.1",
        "league/commonmark": "^2.3.9",
        "league/flysystem-aws-s3-v3": "^3.0",
        "livewire/livewire": "^2.12.3",
        "maatwebsite/excel": "^3.1.48",
        "mews/purifier": "^3.4",
        "mint/service": "dev-master",
        "mpdf/mpdf": "^8.1.4",
        "opcodesio/log-viewer": "^2.3.1",
        "predis/predis": "^2.1.2",
        "pusher/pusher-php-server": "^7.2.2",
        "spatie/laravel-activitylog": "^4.7.3",
        "spatie/laravel-backup": "^8.1.7",
        "spatie/laravel-permission": "^5.9.1",
        "spatie/valuestore": "^1.3.2",
        "symfony/http-client": "^6.2.7",
        "symfony/mailgun-mailer": "^6.2.7",
        "ua-parser/uap-php": "^3.9.14"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.8.1",
        "fakerphp/faker": "^1.21",
        "laravel/pint": "^1.6",
        "laravel/telescope": "^4.14.0",
        "mockery/mockery": "^1.5.1",
        "nunomaduro/collision": "^7.0",
        "pestphp/pest": "^2.0",
        "pestphp/pest-plugin-faker": "^2.0",
        "pestphp/pest-plugin-laravel": "^2.0",
        "pestphp/pest-plugin-mock": "^2.0",
        "spatie/pest-plugin-test-time": "^2.0",
        "spatie/laravel-ignition": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "laravel/telescope"
            ]
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "phpstan/extension-installer": true,
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
